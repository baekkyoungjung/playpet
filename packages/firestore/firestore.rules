rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // all allow
    match /{document=**} {
      allow read, write: if request.time < timestamp.date(2020, 8, 26);
    }
    
    // production
    // match /{document=**} {
    //   allow read, write: if false;
    // }
  }
}